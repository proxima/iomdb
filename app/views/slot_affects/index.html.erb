<table>
  <% for slot_affect in @slot_affects %>
  <tr>
    <td><%=h EquipmentPiece.find_by_id(slot_affect.equipment_piece_id).name %></td>
    <td><%=h Slot.find_by_id(slot_affect.slot_id).name %></td>
    <td><%= link_to 'Show', slot_affect %></td>
    <td><%= link_to 'Edit', edit_slot_affect_path(slot_affect) %></td>
    <td><%= link_to 'Destroy', slot_affect, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br>

<%= link_to 'New', new_slot_affect_path %>
